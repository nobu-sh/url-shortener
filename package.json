{
  "name": "serverless-react-boilerplate",
  "version": "1.0.0",
  "description": "Serverless React Webpack Boilerplate",
  "homepage": "https://github.com/NobUwU/serverless-react-boilerplate",
  "author": "Nobu",
  "license": "MIT",
  "repository": "https://github.com/NobUwU/serverless-react-boilerplate",
  "scripts": {
    "start": "node dist",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && webpack serve --mode=development",
    "dev:server": "nodemon --config nodemon.json server/index.ts",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd client && cross-env NODE_ENV=production webpack --mode production",
    "build:server": "cross-env NODE_ENV=production tsc -p tsconfig.json",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "eslint \"server/**.{ts,tsx}\"",
    "lint:client": "eslint \"client/src/**.{ts,tsx}\"",
    "seed": "ts-node server/database/seedDb.ts"
  },
  "dependencies": {
    "argon2": "^0.30.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-rate-limit": "^6.6.0",
    "i": "^0.3.7",
    "joi": "^17.6.3",
    "jsonwebtoken": "^8.5.1",
    "npm": "^8.19.2",
    "recoil": "^0.7.6",
    "sequelize": "^6.25.0",
    "sqlite3": "^5.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bluebird": "^3.5.36",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-typing-effect": "^2.0.3",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.6.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-typing-effect": "^2.0.5",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3"
  }
}
